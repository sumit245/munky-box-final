{
  "expo": {
    "name": "Feasti",
    "slug": "feasti",
    "version": "0.0.1",
    "orientation": "portrait",
    "plugins":[
      [
        "expo-notifications",
        {
          "icons":"./assets/adaptive-icon.png"
        }

    ]
    ],
     "splash": {   
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.feast.feasti-user"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.srpgroup.munkybox",
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleSignIn": {
          "certificateHash": "1d:47:2b:8b:d9:11:1e:ad:06:66:d0:9f:09:eb:c0:a8:3e:f0:19:65:78:fa:3a:aa:17:fa:59:00:bf:73:55:02"
        }
      },
      "useNextNotificationsApi":true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}